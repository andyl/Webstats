<h1 style='padding-bottom: 15px;'>Users</h1>

<p>
  <a href='/users/new'>create</a>
</p>


<table class='table table-sm'>
  <%= for user <- @users do %>
    <tr>
      <td><%= render "user.html", user: user %></td>
      <td><%= link "View", to: Routes.user_path(@conn, :show, user.id) %></td>
    </tr>
  <% end %>
</table>
